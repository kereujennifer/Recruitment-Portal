@model Auth.Models.ViewModels.Auth.ResetPasswordVW;
@{
    ViewData["Title"] = "ResetPassword";
    Layout = "~/Views/Shared/_AuthLayout.cshtml";
}

<partial name="_LogoHeaderPartial" />

<form method="post">
    <input asp-for="Email" />
    <input asp-for="Token" />
    <div class="form-group">
        <ion-icon name="lock-closed-outline"></ion-icon>
        <input class="form-control" asp-for="Password" type="password" placeholder="Password" />
        <span asp-validation-for="Password"></span>
    </div>

    <div class="form-group">
        <ion-icon name="lock-open-outline"></ion-icon>
        <input class="form-control" asp-for="ConfirmPassword" type="password" placeholder="Confirm Password" />
        <span asp-validation-for="ConfirmPassword"></span>
    </div>

    <button class="btn primary w-100">Submit</button>
</form>



